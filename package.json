{
  "name": "React-Starter",
  "version": "1.0.0",
  "scripts": {
    "sass": "node-sass app/style.scss app/style.css --output-style expanded",
    "browserify": "browserify app/app.js -t [ babelify --presets [ react ] ] -o app/app.min.js",
    "uglify": "uglifyjs app/app.min.js -o app/app.min.js",
    "js": "npm run browserify && npm run uglify",
    "watch:sass": "fsmonitor -d app/ -q '+*.scss' '!app/style.scss' npm run -s sass",
    "watch:js": "fsmonitor -d app/ -q '+*.js' '!app.min.js' npm run js",
    "browsersync": "browser-sync start --files 'app/*, !app/**/*.scss, !app/app.js, !.git/*, app/**/*' --no-open --proxy localhost/portfolio",
    "watch": "npm run -s watch:sass & npm run watch:js & npm run browsersync"
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "fsmonitor": "^0.2.4",
    "node-sass": "^3.10.1",
    "prop-types": "^15.5.10",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-preload": "^0.5.1",
    "react-router": "^2.8.1",
    "react-router-dom": "^4.1.2",
    "react-slick": "^0.15.4",
    "uglify": "^0.1.5",
    "whatwg-fetch": "^2.0.3"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  }
}
